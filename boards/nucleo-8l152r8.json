{
  "build": {
    "core": "sduino",
    "extra_flags": "-DNUCLEO_8L152R8 -DSTM8L152R8",
    "f_cpu": "16000000L",
    "cpu": "stm8l15x",
    "mcu": "stm8l152R8",
    "variant": "stm8lnucleo"
  },
  "debug": {
    "openocd_target": "stm8l",
    "tools": {
      "stlink": {
        "default": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/stlink.cfg",
            "-f",
            "scripts/target/stm8l.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        },
        "onboard": true
      }
    }
  },
  "frameworks": [
    "spl"
  ],
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 32768,
    "protocol": "stlink",
    "protocols": [
      "serial",
      "stlink",
      "stlinkv2"
    ]
  },
  "name": "ST NUCLEO-8L152R8",
  "url": "https://www.st.com/en/evaluation-tools/stm8s-discovery.html",
  "vendor": "ST"
}
